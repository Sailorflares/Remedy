<div class="col-md-4">
  <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title">How are you feeling today?</h3>
    </div>
    <div class="panel-body">
      <%= form_for @feeling, :url => create_feeling_path, :html => {:method => :post} do |f| %>
        <%= f.label :rating, "Rating:" %>
        <%= f.select :rating, (1..10)%>
        <%= f.submit %>
      <% end %>
    </div>
  </div>
</div>